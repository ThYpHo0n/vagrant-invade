config.vm.define "<%= machine_name %>" do |<%= machine_name %>|

  <% if vm %>
    # VM configuration
  <%= vm %>
  <% end %>
  <% if network %>
    # Network configuration
  <%= network %>
  <% end %>
  <% if provider %>
    # Provider configuration
  <%= provider %>
  <% end %>
  <% if synced_folder %>
    # Synced folder configuration
  <%= synced_folder %>
  <% end %>
  <% if plugin %>
    # Plugin configuration
  <%= plugin %>
  <% end %>
  end
